alias zshrc='vi /Users/xxxxx/.zshrc'
alias c='clear'
alias ls='ls -la --color=auto'
alias ..='cd ..'
alias ..2='cd ../..'
alias ..3='cd ../../..'
alias ..4='cd ../../../..'
alias g='git'
alias glo='git log --oneline'
alias ga='git add'
alias gd='git diff'
alias gs='git status'
alias gp='git push'
alias gpf='git push -f'
alias gb='git branch'
alias gc='git checkout'
alias gcb='git checkout -b'
alias gcm='git commit -m'
alias gcp='git cherry-pick'
alias gf='git fetch'
alias grb='git fetch; git rebase origin/master; git log -1 --oneline'
alias grh='git reset --hard'
alias grs='git reset --soft'
alias gfm='git format-patch -o ~/Documents/patch'
alias gam='git am ~/Documents/patch/*.patch'
alias dup='docker-compose up -d'
alias dub='docker-compose up -d --build'
alias rmd='find . -name ".DS_Store" | xargs rm'
alias rmg='rm -ri .git'